         	<contentType id="org.eclipse.wst.xml.core.xmlsource">
           		<partitionType id="org.jboss.tools.common.text.ext.xml.XML_ATTRIBUTE_VALUE">
         			<axis path="*/jsfid/" />
         			<axis path="/view/component/extends/" />
         		</partitionType>
         	</contentType>
         	<contentType id="org.eclipse.core.runtime.xml">
           		<partitionType id="org.jboss.tools.common.text.ext.xml.XML_ATTRIBUTE_VALUE">
         			<axis path="*/jsfid/" />

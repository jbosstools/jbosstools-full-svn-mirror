  <XEntityRenderer>
   <ICONS>
    <ICON info="main.esb.file" type="main"/>
   </ICONS>
  </XEntityRenderer>
  <XModelAttributes>
   <XModelAttribute default="JBoss esb" loader="ElementType" name="element type">

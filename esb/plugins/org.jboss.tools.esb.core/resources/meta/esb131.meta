     HandlerClassName="org.jboss.tools.jsf.model.handlers.RenameFacesConfigHandler"
     ICON="action.empty" PROPERTIES="validator.edit=true"
     WizardClassName="%Default%" displayName="Rename..." kind="action" name="Rename">

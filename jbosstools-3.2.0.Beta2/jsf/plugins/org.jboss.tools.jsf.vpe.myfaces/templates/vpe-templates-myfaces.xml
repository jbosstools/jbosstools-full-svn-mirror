			</vpe:dnd>
			<vpe:resize>
				<vpe:width width-attr="style.width" />
				<vpe:height height-attr="style.height" />
			</vpe:resize>
			<vpe:textFormatting use-default-formats="yes">
			</vpe:textFormatting>
		</vpe:template>
	</vpe:tag>



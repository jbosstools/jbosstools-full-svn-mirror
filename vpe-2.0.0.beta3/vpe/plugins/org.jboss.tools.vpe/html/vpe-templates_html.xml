    <vpe:tag name="input" case-sensitive="no">
		<vpe:if test="(@type='')|(@type='text')">
			<vpe:template children="no" modify="no">
				<vpe:copy attrs="type,style,class,value,size"/>
			</vpe:template>
		</vpe:if>
		<vpe:if test="@type='password'">
		    <vpe:template children="no" modify="no">
				<vpe:copy attrs="type,style,class,value,size"/>
		    </vpe:template>
		</vpe:if>
		<vpe:if test="(@type='checkbox')|(@type='radio')">
		    <vpe:template children="no" modify="no">
				<vpe:copy attrs="style,class,type,checked">
					<!--vpe:attribute name="disabled" value="disabled"/-->
				</vpe:copy>
		    </vpe:template>
		</vpe:if>
		<vpe:if test="(@type='submit')|(@type='button')">
		    <vpe:template children="no" modify="no">
				<vpe:copy attrs="type,value,style,class">
					<!--vpe:attribute name="disabled" value="disabled"/-->
				</vpe:copy>
		    </vpe:template>
		</vpe:if>
		<vpe:if test="@type='reset'">
			<vpe:if test="@value=''">
			    <vpe:template children="no" modify="no">
					<vpe:copy attrs="type,checked,class,style">
						<vpe:attribute name="value" value="Reset"/>
						<!--vpe:attribute name="disabled" value="disabled"/-->
					</vpe:copy>
			    </vpe:template>
			</vpe:if>
			<vpe:if test="not(@value='')">
			    <vpe:template children="no" modify="no">
					<vpe:copy attrs="type,checked,value,class,style">
						<!--vpe:attribute name="disabled" value="disabled"/-->
					</vpe:copy>
			    </vpe:template>
			</vpe:if>
		</vpe:if>
		<vpe:if test="@type='file'">
		    <vpe:template children="no" modify="yes">
		    </vpe:template>
		</vpe:if>
		<vpe:if test="@type='hidden'">
		    <vpe:template children="no" modify="no">
				<input type="text" value="{@value}" class="{@class}" size="{@size}" style="{@style}border-style:dotted;"/>
		    </vpe:template>
		</vpe:if>
		<vpe:if test="@type='image'">
		    <vpe:template children="no" modify="no">
				<img src="{@src}" width="20" height="20"/>
		    </vpe:template>
		</vpe:if>
    </vpe:tag>


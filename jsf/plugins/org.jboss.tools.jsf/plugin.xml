      <keys path="help/keys-jsf-menu.properties"/>
      <keys path="help/keys-jsf-lists.properties"/>
   </extension>
   <extension id="jsfnature" point="org.eclipse.core.resources.natures">
      <runtime>
         <run class="org.jboss.tools.jsf.project.JSFNature">
         </run>
         <library name="$nl$/"/>         
      </runtime>
   </extension>


<?xml version="1.0" encoding="UTF-8"?>
<smooks-resource-list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd" xmlns:edi="http://www.milyn.org/xsd/smooks/edi-1.1.xsd"
	xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.1.xsd"
	xmlns:g="http://www.jboss.org/jbosstools/smooks/smooks-graphics-ext.xsd">
	<params>
		<param name="stream.filter.type">SAX</param>
	</params>
	<abstract-reader xsi:type="edi:reader" encoding="UTF-8"
		mappingModel="/org/jboss/tools/smooks/test/ediparser/edi-to-xml-order-mapping.xml" />
	<jb:bindings beanId="a" class="example.A">
		<jb:value data="a" property="name" />
		<jb:wiring beanIdRef="a" property="b" />
	</jb:bindings>

	<g:smooks-graphics-ext
		>
		<g:input type="EDI">
			<g:param name="path">Workspace://javabean_test/src/example/input-message.edi
			</g:param>
			<g:param name="use_availableReader">true</g:param>
		</g:input>
		<g:input type="EDI">
			<g:param name="path">Workspace://javabean_test/src/example/input-message.edi
			</g:param>
			<g:param name="encoding">UTF-8</g:param>
			<g:param name="mappingModelFile">/org/jboss/tools/smooks/test/ediparser/edi-to-xml-order-mapping.xml
			</g:param>
		</g:input>
	</g:smooks-graphics-ext>
</smooks-resource-list>
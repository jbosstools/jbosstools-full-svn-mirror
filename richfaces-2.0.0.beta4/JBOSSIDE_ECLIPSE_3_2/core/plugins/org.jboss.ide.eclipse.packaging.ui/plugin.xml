<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension
         point="org.eclipse.ui.propertyPages">
      <page
            adaptable="true"
            objectClass="org.eclipse.jdt.core.IJavaProject"
            name="%org.jboss.ide.eclipse.packaging.ui.properties.PackagingPropertyPage"
            class="org.jboss.ide.eclipse.packaging.ui.properties.PackagingPropertyPage"
            id="org.jboss.ide.eclipse.packaging.ui.properties.PackagingPropertyPage">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            name="%org.jboss.ide.eclipse.packaging.ui.commands.PackagingRunAction"
            categoryId="org.eclipse.ui.category.project"
            id="org.jboss.ide.eclipse.packaging.ui.commands.PackagingRunAction">
      </command>
      <keyBinding
            commandId="org.jboss.ide.eclipse.packaging.ui.commands.PackagingRunAction"
            keySequence="Shift+Ctrl+F2"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="%org.jboss.ide.eclipse.packaging.ui.actions.Packaging"
            visible="true"
            id="org.jboss.ide.eclipse.packaging.ui.actions.Packaging">
         <action
               definitionId="org.jboss.ide.eclipse.packaging.ui.commands.PackagingRunAction"
               label="%org.jboss.ide.eclipse.packaging.ui.actions.PackagingRunAction"
               icon="icons/full/obj16/run.gif"
               tooltip="Run Packaging on current Project"
               class="org.jboss.ide.eclipse.packaging.ui.actions.PackagingRunAction"
               menubarPath="project/additions"
               id="org.jboss.ide.eclipse.packaging.ui.actions.PackagingRunAction">
            <enablement>
               <objectState
                     name="sessionProperty"
                     value="org.jboss.ide.eclipse.packaging.enabled=true"/>
            </enablement>
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            objectClass="org.eclipse.core.resources.IProject"
            id="org.jboss.ide.eclipse.packaging.ui.popup.Packaging">
         <action
               definitionId="org.jboss.ide.eclipse.packaging.ui.commands.PackagingRunAction"
               label="%org.jboss.ide.eclipse.packaging.ui.popup.PackagingRunAction"
               icon="icons/full/obj16/run.gif"
               class="org.jboss.ide.eclipse.packaging.ui.actions.PackagingRunAction"
               menubarPath="group.build"
               enablesFor="1"
               id="org.jboss.ide.eclipse.packaging.ui.popup.PackagingRunAction">
            <enablement>
               <objectState
                     name="sessionProperty"
                     value="org.jboss.ide.eclipse.packaging.enabled=true"/>
            </enablement>
         </action>
      </objectContribution>
   </extension>

</plugin>

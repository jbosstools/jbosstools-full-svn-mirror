<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category id="org.jboss.tools..shale" name="Shale" parentCategory="org.jboss.tools.jst.web">
      </category>
      <wizard 
         category="org.jboss.tools.jst.web/org.jboss.tools.shale" 
         class="org.jboss.tools.shale.ui.wizard.newfile.NewShaleDialogConfigFileWizard" 
         icon="images/xstudio/wizards/dialog-config.gif" 
         id="org.jboss.tools.shale.ui.wizard.newfile.NewShaleDialogConfigFileWizard" 
         name="Shale Dialog Config 1.0" 
         project="false">
         <selection class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
      <wizard 
         category="org.jboss.tools.jst.web/org.jboss.tools.shale" 
         class="org.jboss.tools.shale.ui.wizard.newfile.NewClayConfigFileWizard" 
         icon="images/xstudio/wizards/clay-config.gif" 
         id="org.jboss.tools.shale.ui.wizard.newfile.NewClayConfigFileWizard" 
         name="Clay Config 1.0" 
         project="false">
         <selection class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
   </extension>

   <extension id="e" name="XML Editors" point="org.jboss.tools.common.model.ui.xmlEditor">
      <xmlEditor 
            class="org.jboss.tools.jst.web.ui.editors.WebCompoundEditor" 
            contributorClass="org.jboss.tools.common.model.ui.texteditors.MultiPageContributor" 
            entities="FileShaleDialog" 
            icon="images/xstudio/editors/dialog-config.gif" 
            name="%editors.shale-dialog.name">
      </xmlEditor>
      <xmlEditor 
            class="org.jboss.tools.shale.ui.clay.editor.ClayCompoundEditor" 
            contributorClass="org.jboss.tools.shale.ui.clay.editor.action.ClayMultiPageContributor" 
            entities="FileShaleClay" 
            icon="images/xstudio/editors/clay-config.gif" 
            name="%editors.shale-clay.name">
      </xmlEditor>
      <xmlEditor 
            class="org.jboss.tools.jst.web.ui.editors.WebCompoundEditor" 
            contributorClass="org.jboss.tools.common.model.ui.texteditors.MultiPageContributor" 
            entities="FileSpringBeans" 
            icon="images/xstudio/editors/xml_file.gif" 
            name="%editors.spring-beans.name">
      </xmlEditor>
   </extension>

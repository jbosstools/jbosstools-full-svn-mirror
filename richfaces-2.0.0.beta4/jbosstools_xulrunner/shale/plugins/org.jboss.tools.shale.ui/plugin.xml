   <extension point="org.jboss.tools.common.model.ui.attributeAdapter">
      <attributeAdapter class="org.jboss.tools.shale.ui.attribute.adapter.ShaleStateListAdapter" id="ShaleStateList">
      </attributeAdapter>
      <attributeAdapter class="org.jboss.tools.shale.ui.attribute.adapter.ShaleDialogListAdapter" id="ShaleDialogList">
      </attributeAdapter>
      <attributeAdapter class="org.jboss.tools.shale.ui.attribute.adapter.ClayComponentListAdapter" id="ClayComponentList">
      </attributeAdapter>
      
   </extension>
   <extension point="org.jboss.tools.common.model.ui.attributeEditor">
      <attributeEditor class="org.jboss.tools.common.model.ui.attribute.editor.ComboBoxEditor" id="ShaleStateList">
      </attributeEditor>
      <attributeEditor class="org.jboss.tools.common.model.ui.attribute.editor.ComboBoxEditor" id="ShaleDialogList">
      </attributeEditor>
      <attributeEditor class="org.jboss.tools.common.model.ui.attribute.editor.ComboBoxEditor" id="ClayComponentList">
      </attributeEditor>
   </extension>

   <extension
         point="org.jboss.tools.common.model.resourceMapping">
      <mapping
          loader="org.jboss.tools.shale.ui"
          priority="1"
          kind="class"
          paths=".jboss.tools.shale."
      />
   </extension>

   <extension point="org.jboss.tools.common.model.classes">
		<xclass id="org.jboss.tools.shale.ui.clay.editor.ClayEditorMessages"
			class="org.jboss.tools.shale.ui.clay.editor.ClayEditorMessages"/>
		<xclass id="org.jboss.tools.shale.ui.clay.form.ShaleClayFormLayoutData"
			class="org.jboss.tools.shale.ui.clay.form.ShaleClayFormLayoutData"/>
		<xclass id="org.jboss.tools.shale.ui.dialog.form.ShaleDialogFormLayoutData"
			class="org.jboss.tools.shale.ui.dialog.form.ShaleDialogFormLayoutData"/>
		<xclass id="org.jboss.tools.shale.ui.spring.form.SpringFormLayoutData"
			class="org.jboss.tools.shale.ui.spring.form.SpringFormLayoutData"/>
   </extension>

</plugin>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="org.hibernate.netbeans.console.model.Store" table="hc_store">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="name2"/>
        <many-to-one name="city" class="org.hibernate.netbeans.console.model.StoreCity"/>
    </class>
        
</hibernate-mapping>
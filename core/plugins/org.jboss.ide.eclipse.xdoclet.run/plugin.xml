<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension
         point="org.eclipse.ui.propertyPages">
      <page
            adaptable="true"
            objectClass="org.eclipse.jdt.core.IJavaProject"
            name="%org.jboss.ide.eclipse.xdoclet.run.ui.properties.ConfigurationPropertyPage"
            class="org.jboss.ide.eclipse.xdoclet.run.ui.properties.ConfigurationPropertyPage"
            id="org.jboss.ide.eclipse.xdoclet.run.ui.properties.ConfigurationPropertyPage">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            name="%org.jboss.ide.eclipse.xdoclet.run.ui.commands.XDocletRunAction"
            categoryId="org.eclipse.ui.category.project"
            id="org.jboss.ide.eclipse.xdoclet.run.ui.commands.XDocletRunAction">
      </command>
      <keyBinding
            commandId="org.jboss.ide.eclipse.xdoclet.run.ui.commands.XDocletRunAction"
            keySequence="Shift+Ctrl+F1"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="%org.jboss.ide.eclipse.xdoclet.run.ui.actions.XDoclet"
            visible="true"
            id="org.jboss.ide.eclipse.xdoclet.run.ui.actions.XDoclet">
         <action
               definitionId="org.jboss.ide.eclipse.xdoclet.run.ui.commands.XDocletRunAction"
               label="%org.jboss.ide.eclipse.xdoclet.run.ui.actions.XDocletRunAction"
               icon="icons/full/obj16/run.gif"
               tooltip="Run XDoclet on current Project"
               class="org.jboss.ide.eclipse.xdoclet.run.ui.actions.XDocletRunAction"
               menubarPath="project/additions"
               id="org.jboss.ide.eclipse.xdoclet.run.ui.actions.XDocletRunAction">
            <enablement>
               <objectState
                     name="sessionProperty"
                     value="org.jboss.ide.eclipse.xdoclet.enabled=true"/>
            </enablement>
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            objectClass="org.eclipse.core.resources.IProject"
            id="org.jboss.ide.eclipse.xdoclet.run.ui.popup.XDoclet">
         <action
               definitionId="org.jboss.ide.eclipse.xdoclet.run.ui.commands.XDocletRunAction"
               label="%org.jboss.ide.eclipse.xdoclet.run.ui.popup.XDocletRunAction"
               icon="icons/full/obj16/run.gif"
               class="org.jboss.ide.eclipse.xdoclet.run.ui.actions.XDocletRunAction"
               menubarPath="group.build"
               enablesFor="1"
               id="org.jboss.ide.eclipse.xdoclet.run.ui.popup.XDocletRunAction">
            <enablement>
               <objectState
                     name="sessionProperty"
                     value="org.jboss.ide.eclipse.xdoclet.enabled=true"/>
            </enablement>
         </action>
      </objectContribution>
   </extension>
   <extension
         id="XDocletRunBuilder"
         name="JBoss Eclipse IDE XDoclet Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="false"
            isConfigurable="true">
         <run class="org.jboss.ide.eclipse.xdoclet.run.builder.XDocletRunBuilder"/>
      </builder>
   </extension>
</plugin>

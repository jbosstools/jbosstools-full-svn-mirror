<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.jboss.ide.eclipse.jdt.j2ee.jsp.core"
   name="%plugin.name"
   version="1.5.0.m1"
   provider-name="%plugin.provider"
   class="org.jboss.ide.eclipse.jdt.j2ee.jsp.core.JDTJ2EEJSPCorePlugin">

   <runtime>
      <library name="org-jboss-ide-eclipse-jdt-j2ee-jsp-core.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-el.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-logging.jar">
         <export name="*"/>
      </library>
      <library name="lib/jsp-api.jar"/>
      <library name="lib/servlet-api.jar"/>
   </runtime>

   <requires>
      <import plugin="org.jboss.ide.eclipse.core"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.jboss.ide.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.apache.ant"/>
      <import plugin="org.apache.xalan"/>
      <import plugin="org.jboss.ide.eclipse.jdt.j2ee.core"/>
      <import plugin="org.jboss.ide.eclipse.jdt.test.core"/>
      <import plugin="org.jboss.ide.eclipse.jdt.xml.core"/>
   </requires>

   <extension
         id="jsp-nature"
         name="JSP Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.jboss.ide.eclipse.jdt.j2ee.jsp.core.JSPProjectNature">
         </run>
      </runtime>
      <requires-nature
            id="org.eclipse.jdt.core.javanature">
      </requires-nature>
      <builder
            id="org.jboss.ide.eclipse.jdt.j2ee.jsp.core.jsp-builder">
      </builder>
   </extension>
   <extension
         id="jsp-builder"
         name="JSP Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="org.jboss.ide.eclipse.jdt.j2ee.jsp.core.compiler.JSPBuilder">
         </run>
      </builder>
   </extension>

</plugin>

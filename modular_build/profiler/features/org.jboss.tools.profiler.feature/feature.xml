<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.jboss.tools.profiler.feature"
      label="%featureName"
      version="1.0.0.qualifier"
      provider-name="%providerName">

   <description url="http://www.jboss.org/tools">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>


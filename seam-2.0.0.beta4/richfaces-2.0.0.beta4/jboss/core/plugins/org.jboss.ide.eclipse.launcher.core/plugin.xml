<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.jboss.ide.eclipse.launcher.core"
   name="%plugin.name"
   version="1.5.0.m1"
   provider-name="%plugin.provider"
   class="org.jboss.ide.eclipse.launcher.core.LauncherPlugin">

   <runtime>
      <library name="org-jboss-ide-eclipse-launcher-core.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.eclipse.jdt"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt.launching"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.jboss.ide.eclipse.core"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.apache.ant"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
   </requires>

   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            sourceLocatorId="org.eclipse.jdt.launching.sourceLocator.JavaSourceLookupDirector"
            name="%org.jboss.ide.eclipse.launcher.core.configuration.jboss.JBoss24xLaunchConfigurationDelegate"
            sourcePathComputerId="org.eclipse.jdt.launching.sourceLookup.javaSourcePathComputer"
            delegate="org.jboss.ide.eclipse.launcher.core.configuration.jboss.JBoss24xLaunchConfigurationDelegate"
            modes="debug"
            id="org.jboss.ide.eclipse.launcher.configuration.jboss.JBoss24xLaunchConfigurationDelegate">
      </launchConfigurationType>
      <launchConfigurationType
            sourceLocatorId="org.eclipse.jdt.launching.sourceLocator.JavaSourceLookupDirector"
            name="%org.jboss.ide.eclipse.launcher.core.configuration.jboss.JBoss30xLaunchConfigurationDelegate"
            sourcePathComputerId="org.eclipse.jdt.launching.sourceLookup.javaSourcePathComputer"
            delegate="org.jboss.ide.eclipse.launcher.core.configuration.jboss.JBoss30xLaunchConfigurationDelegate"
            modes="debug"
            id="org.jboss.ide.eclipse.launcher.configuration.jboss.JBoss30xLaunchConfigurationDelegate">
      </launchConfigurationType>
      <launchConfigurationType
            sourceLocatorId="org.eclipse.jdt.launching.sourceLocator.JavaSourceLookupDirector"
            name="%org.jboss.ide.eclipse.launcher.core.configuration.jboss.JBoss32xLaunchConfigurationDelegate"
            sourcePathComputerId="org.eclipse.jdt.launching.sourceLookup.javaSourcePathComputer"
            delegate="org.jboss.ide.eclipse.launcher.core.configuration.jboss.JBoss32xLaunchConfigurationDelegate"
            modes="debug"
            id="org.jboss.ide.eclipse.launcher.configuration.jboss.JBoss32xLaunchConfigurationDelegate">
      </launchConfigurationType>
      <launchConfigurationType
            sourceLocatorId="org.eclipse.jdt.launching.sourceLocator.JavaSourceLookupDirector"
            name="%org.jboss.ide.eclipse.launcher.core.configuration.jboss.JBoss40xLaunchConfigurationDelegate"
            sourcePathComputerId="org.eclipse.jdt.launching.sourceLookup.javaSourcePathComputer"
            delegate="org.jboss.ide.eclipse.launcher.core.configuration.jboss.JBoss40xLaunchConfigurationDelegate"
            modes="debug"
            id="org.jboss.ide.eclipse.launcher.configuration.jboss.JBoss40xLaunchConfigurationDelegate">
      </launchConfigurationType>
      <launchConfigurationType
            sourceLocatorId="org.eclipse.jdt.launching.sourceLocator.JavaSourceLookupDirector"
            name="%org.jboss.ide.eclipse.launcher.core.configuration.RemoteServerLaunchConfigurationDelegate"
            sourcePathComputerId="org.eclipse.jdt.launching.sourceLookup.javaSourcePathComputer"
            delegate="org.jboss.ide.eclipse.launcher.core.configuration.RemoteServerLaunchConfigurationDelegate"
            modes="debug"
            id="org.jboss.ide.eclipse.launcher.configuration.RemoteServerLaunchConfigurationDelegate">
      </launchConfigurationType>
   </extension>

</plugin>
